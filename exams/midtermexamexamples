import java.util.*;
import javax.swing.*;

public class midTermexam {

	public static void main(String[] args) {
		int songs = 0;
		double total = 0;

		while (total <= 0) {
		songs = Integer.parseInt(JOptionPane.showInputDialog("How many songs? "));

		if (songs < 10 && songs >= 0) {
			total = 1.20 * songs;
			JOptionPane.showMessageDialog(null,"Total price is " + total,"", JOptionPane.INFORMATION_MESSAGE);
		}
		else if (songs >= 10 && songs <= 50) {
			total = 1.00 * songs;
			JOptionPane.showMessageDialog(null,"Total price is " + total,"", JOptionPane.INFORMATION_MESSAGE);
			
		}
		else if (songs > 50) {
			total = songs * 0.90;
			JOptionPane.showMessageDialog(null,"Total price is " + total,"", JOptionPane.INFORMATION_MESSAGE);
		}
		else {
			JOptionPane.showMessageDialog(null,"Error please enter value","", JOptionPane.INFORMATION_MESSAGE);
			continue;
			}
		}
	}
}

