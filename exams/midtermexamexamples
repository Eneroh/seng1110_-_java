import java.util.*;

public class midTermexam {

	public static void main(String[] args) {
		Scanner console = new Scanner(System.in);
			double first_item = 0;
			double second_item = 0;
			double total = 0;
			
			System.out.print("Price of item 1 = ");
			first_item = console.nextDouble();
			System.out.print("Price of item 2 = ");
			second_item = console.nextDouble();
			
			total = first_item + second_item;

			System.out.print("Total price is " + total);
	}
}

