import java.util.*;
import javax.swing.*;

public class midTermexam {

	public static void main(String[] args) {
			double first_item = 0;
			double second_item = 0;
			double total = 0;
		
			first_item = Double.parseDouble(JOptionPane.showInputDialog("first_item"));
			second_item = Double.parseDouble(JOptionPane.showInputDialog("second_item"));
				
			total = first_item + second_item;

			JOptionPane.showMessageDialog(null,"Total price is " + total,"",JOptionPane.INFORMATION_MESSAGE);
	}
}

