import java.util.*;

public class midTermexam {

	public static void main(String[] args) {
		Scanner console = new Scanner(System.in);
			int x = 0;
			int y = 10;
			String hi = "test";
			char result = hi.charAt(0);
			for(x = 1; x < y; x = x * 2) {
				System.out.println(x);
				System.out.println(result);
				System.out.println(hi);
			}
	}
}

